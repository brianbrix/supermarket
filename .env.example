# Brand configuration (customize per deployment)
# Copy to .env (never commit secrets) and adjust values
# Accessed in code via import.meta.env.* (Vite)

VITE_BRAND_NAME=Shoppers Paradise
VITE_BRAND_TAGLINE=Fresh & Local
# Optional: Title for receipts (defaults to "${VITE_BRAND_NAME} Market")
VITE_BRAND_RECEIPT_TITLE=Shoppers Paradise Market

# API base URL consumed by frontend fetch layer (adjust port / host per environment)
VITE_API_BASE_URL=http://localhost:8081/api
